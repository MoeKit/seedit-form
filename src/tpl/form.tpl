<div id="{{id}}" style="height:100%;background:#f6f6f6">
	<div class="personal-info-tips personal-info-add">{{addTips}}</div>
	<div class="personal-info-tips personal-info-edit">{{editTips}}</div>
	<ul class="personal-info-list">{{body}}</ul>
	<div class="personal-info-tips2">{{tips}}</div>
	<button type="button" class="personal-info-btn personal-btn-edit">{{editBtn}}</button>
	<button type="button" class="personal-info-btn personal-btn-add">{{addBtn}}</button>
	<button type="button" class="personal-info-btn personal-btn-return">{{returnList}}</button>
	<button type="button" class="personal-info-btn personal-btn-return2">{{returnList}}</button>
	<button type="button" class="personal-info-btn personal-new-close">{{colBtn}}</button>
	<button type="button" class="personal-info-btn personal-btn-delet">{{delBtn}}</button>
	<div class="opacity-bg hide"></div>
	<div class="personal-delet-confirm hide">
		<div class="confirm-text">
			<div class="personal-delet-title">{{delTips}}</div>
			<button type="button" class="personal-delet-btn delet-btn-no">{{cancle}}</button>
			<button type="button" class="personal-delet-btn delet-btn-yes">{{delBtnYes}}	</button>
		</div>
	</div>
